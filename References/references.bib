@misc{bakhtiExploringSolarNeutrino2023,
  title = {Exploring {{Solar Neutrino Oscillation Parameters}} with {{LCS}} at {{Yemilab}} and {{JUNO}}},
  author = {Bakhti, Pouya and Rajaee, Meshkat and Seo, Seon-Hee and Shin, Seodong},
  year = {2023},
  month = jul,
  number = {arXiv:2307.11582},
  eprint = {2307.11582},
  primaryclass = {hep-ex, physics:hep-ph},
  publisher = {{arXiv}},
  urldate = {2023-07-24},
  abstract = {We investigate the sensitivities of the liquid scintillator counter (LSC) at Yemilab and JUNO to solar neutrino oscillation parameters, focusing on \${\textbackslash}theta\_\{12\}\$ and \${\textbackslash}Delta m\^2\_\{21\}\$. We compare the potential of JUNO with LSC at Yemilab utilizing both reactor and solar data in determining those parameters. We find that the solar neutrino data of LSC at Yemilab is highly sensitive to \${\textbackslash}theta\_\{12\}\$ enabling its determination with exceptional precision. Our study also reveals that if \${\textbackslash}Delta m\^2\_\{21\}\$ is larger, with a value close to the best fit value of KamLAND, JUNO reactor data will have about two times better precision than the reactor LSC at Yemilab. On the other hand, if \${\textbackslash}Delta m\^2\_\{21\}\$ is smaller and closer to the best fit value of solar neutrino experiments, the precision of the reactor LSC at Yemilab will be comparable/better than JUNO.},
  archiveprefix = {arxiv},
  keywords = {High Energy Physics - Experiment,High Energy Physics - Phenomenology},
  file = {/home/cookmanl/snap/zotero-snap/common/Zotero/storage/9JHKV4G6/Bakhti et al. - 2023 - Exploring Solar Neutrino Oscillation Parameters wi.pdf;/home/cookmanl/snap/zotero-snap/common/Zotero/storage/UAZCVDWG/2307.html}
}

