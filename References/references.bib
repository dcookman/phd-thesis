@misc{abbasStandardHVM2023,
  title = {The Standard {{HVM}}},
  author = {Abbas, Gauhar},
  year = {2023},
  month = oct,
  number = {arXiv:2310.12915},
  eprint = {2310.12915},
  primaryclass = {hep-ex, physics:hep-ph},
  publisher = {{arXiv}},
  doi = {10.48550/arXiv.2310.12915},
  urldate = {2023-11-24},
  abstract = {We discuss the standard hierarchical VEVs model, which predicts the leptonic mixing angles in terms of the Cabibbo angle, and masses of strange and charm quarks as \${\textbackslash}sin {\textbackslash}theta\_\{12\}\^{\textbackslash}ell {\textbackslash}geq 1 - 2 {\textbackslash}sin {\textbackslash}theta\_\{12\}\$, \${\textbackslash}sin {\textbackslash}theta\_\{23\}\^{\textbackslash}ell {\textbackslash}geq 1 - {\textbackslash}sin {\textbackslash}theta\_\{12\}\$, and \${\textbackslash}sin {\textbackslash}theta\_\{13\}\^{\textbackslash}ell {\textbackslash}geq {\textbackslash}sin {\textbackslash}theta\_\{12\} - {\textbackslash}frac\{m\_s\}\{m\_c\}\$ for the normal mass ordering. This results in very precise predictions of the leptonic mixing angles given by \${\textbackslash}sin {\textbackslash}theta\_\{12\}\^{\textbackslash}ell = 0.55 {\textbackslash}pm 0.00134 \$, \${\textbackslash}sin {\textbackslash}theta\_\{23\}\^{\textbackslash}ell = 0.775 {\textbackslash}pm 0.00067 \$, and \${\textbackslash}sin {\textbackslash}theta\_\{13\}\^{\textbackslash}ell = 0.1413 - 0.1509 \$. Furthermore, we predict neutrinos to be the Dirac kind. The flavour bounds on the model are discussed, and the impact of the standard hierarchical VEVs model on the flavonic dark matter is highlighted by showing its natural emergence.},
  archiveprefix = {arxiv},
  keywords = {High Energy Physics - Experiment,High Energy Physics - Phenomenology},
  file = {/home/cookmanl/snap/zotero-snap/common/Zotero/storage/7P93WTUS/Abbas - 2023 - The standard HVM.pdf;/home/cookmanl/snap/zotero-snap/common/Zotero/storage/9J3KTP28/2310.html}
}

