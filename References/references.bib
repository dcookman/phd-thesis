@article{bergstromUpdatedDeterminationSolar2016,
  title = {Updated Determination of the Solar Neutrino Fluxes from Solar Neutrino Data},
  author = {Bergstr{\"o}m, Johannes and {Gonzalez-Garcia}, M. C. and Maltoni, Michele and {Pe{\~n}a-Garay}, Carlos and Serenelli, Aldo M. and Song, Ningqiang},
  year = {2016},
  month = mar,
  journal = {Journal of High Energy Physics},
  volume = {2016},
  number = {3},
  pages = {132},
  issn = {1029-8479},
  doi = {10.1007/JHEP03(2016)132},
  urldate = {2023-07-14},
  abstract = {We present an update of the determination of the solar neutrino fluxes from a global analysis of the solar and terrestrial neutrino data in the framework of three-neutrino mixing. Using a Bayesian analysis we reconstruct the posterior probability distribution function for the eight normalization parameters of the solar neutrino fluxes plus the relevant masses and mixing, with and without imposing the luminosity constraint. We then use these results to compare the description provided by different Standard Solar Models. Our results show that, at present, both models with low and high metallicity can describe the data with equivalent statistical agreement. We also argue that even with the present experimental precision the solar neutrino data have the potential to improve the accuracy of the solar model predictions.},
  langid = {english},
  keywords = {Neutrino Physics,Solar and Atmospheric Neutrinos},
  file = {/home/cookmanl/snap/zotero-snap/common/Zotero/storage/KUCAT4KI/Bergstrm et al. - 2016 - Updated determination of the solar neutrino fluxes.pdf}
}

@article{billerAnotherLookConfidence2015,
  title = {Another Look at Confidence Intervals: {{Proposal}} for a More Relevant and Transparent Approach},
  shorttitle = {Another Look at Confidence Intervals},
  author = {Biller, Steven D. and Oser, Scott M.},
  year = {2015},
  month = feb,
  journal = {Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
  volume = {774},
  pages = {103--119},
  issn = {0168-9002},
  doi = {10.1016/j.nima.2014.11.081},
  urldate = {2023-05-04},
  abstract = {The behaviors of various confidence/credible interval constructions are explored, particularly in the region of low event numbers where methods diverge most. We highlight a number of challenges, such as the treatment of nuisance parameters, and common misconceptions associated with such constructions. An informal survey of the literature suggests that confidence intervals are not always defined in relevant ways and are too often misinterpreted and/or misapplied. This can lead to seemingly paradoxical behaviors and flawed comparisons regarding the relevance of experimental results. We therefore conclude that there is a need for a more pragmatic strategy which recognizes that, while it is critical to objectively convey the information content of the data, there is also a strong desire to derive bounds on model parameter values and a natural instinct to interpret things this way. Accordingly, we attempt to put aside philosophical biases in favor of a practical view to propose a more transparent and self-consistent approach that better addresses these issues.},
  langid = {english},
  keywords = {Bayesian,Confidence intervals,Frequentist,statistics},
  file = {/home/cookmanl/snap/zotero-snap/common/Zotero/storage/5XVFVB62/Biller and Oser - 2015 - Another look at confidence intervals Proposal for.pdf;/home/cookmanl/snap/zotero-snap/common/Zotero/storage/3ML5UUN3/S0168900214013953.html}
}

