  urldate = {2023-05-25},
  abstract = {This paper reports results from a search for single and multi-nucleon disappearance from the \$\^\{16\}\$O nucleus in water within the {\textbackslash}snoplus\{\} detector using all of the available data. These so-called "invisible" decays do not directly deposit energy within the detector but are instead detected through their subsequent nuclear de-excitation and gamma-ray emission. New limits are given for the partial lifetimes: \${\textbackslash}tau(n{\textbackslash}rightarrow inv) {$>$} 9.0{\textbackslash}times10\^\{29\}\$ years, \${\textbackslash}tau(p{\textbackslash}rightarrow inv) {$>$} 9.6{\textbackslash}times10\^\{29\}\$ years, \${\textbackslash}tau(nn{\textbackslash}rightarrow inv) {$>$} 1.5{\textbackslash}times10\^\{28\}\$ years, \${\textbackslash}tau(np{\textbackslash}rightarrow inv) {$>$} 6.0{\textbackslash}times10\^\{28\}\$ years, and \${\textbackslash}tau(pp{\textbackslash}rightarrow inv) {$>$} 1.1{\textbackslash}times10\^\{29\}\$ years at 90{\textbackslash}\% Bayesian credibility level (with a prior uniform in rate). All but the (\$nn{\textbackslash}rightarrow inv\$) results improve on existing limits by a factor of about 3.},
  archiveprefix = {arxiv},
  collaboration = {SNO+ Collaboration},
  keywords = {High Energy Physics - Experiment,SNO+},
  file = {/home/cookmanl/snap/zotero-snap/common/Zotero/storage/3BYY2CYG/Collaboration et al. - 2022 - Improved search for invisible modes of nucleon dec.pdf;/home/cookmanl/snap/zotero-snap/common/Zotero/storage/Z7DEQWH9/2205.html}
}

@article{altmannCompleteResultsFive2005,
  title = {Complete Results for Five Years of {{GNO}} Solar Neutrino Observations},
