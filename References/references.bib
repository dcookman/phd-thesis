  journal = {Optics Express},
  volume = {17},
  number = {3},
  pages = {1671--1678},
  publisher = {{Optica Publishing Group}},
  issn = {1094-4087},
  doi = {10.1364/OE.17.001671},
  urldate = {2023-12-14},
  abstract = {The use of density derivative of the refractive index from the classic Lorentz-Lorenz equation or its variations performed poorly in estimating the scattering by water, leading to the alternative use of pressure derivative instead, which however has been scarcely measured due to its extremely low sensitivity. Recently, density derivative has been deduced directly from theoretical models. Three characterizations of density derivative of the refractive index were evaluated and scattering of water thus calculated converge with each other within 3.5\% and agree with the measurement by Morel (Cahiers Oceanographiques, 20, 157, 1968) within 2\% (with depolarization ratio = 0.039), all improving over the earlier estimates based on either density or pressure derivatives. Taking into account of uncertainty associated with the depolarization ratio, the prediction based on the model by Proutiere et al. (J. Phy. Chem., 96, 3485, 1992) still agrees with the measurement within the experimental errors (2\%).},
  copyright = {{\textcopyright} 2009 Optical Society of America},
  langid = {english},
  keywords = {Anisotropy,Depolarization,Refractive index,Salinity,Scattering measurement,Statistics},
  file = {/home/cookmanl/snap/zotero-snap/common/Zotero/storage/TA5W33YS/Zhang and Hu - 2009 - Estimating scattering of pure water from density f.pdf}
}

